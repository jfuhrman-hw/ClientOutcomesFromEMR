                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
 [SMARTTEXT_NAME] as ContentTitle
,COALESCE(NULLIF(CASE WHEN SMARTTEXT_NAME like '%(ELIQUIS)%' THEN 'ENGLISH' 
	  WHEN SMARTTEXT_NAME like '%(ENOXAPARIN) PT ED%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(BABY FRIENDLY)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(BASE%)' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(LESS THAN ONE)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(MEANINGFUL USE)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(PEDI)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(PLANNING)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '(RETIRED AND UNRELEASED%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(SHORT)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(VAGINAL)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(VAGINAL AND C-SECTION DELIVERIES)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(HIDE)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%(HYPONATREMIA)%' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%_ENGLISH' THEN 'ENGLISH'
	  WHEN SMARTTEXT_NAME like '%_SPANISH' THEN 'SPANISH'
	  WHEN SMARTTEXT_NAME like '%_PORTUGEUSE' THEN 'PORTUGEUSE'
      ELSE REPLACE(REPLACE(RIGHT(smarttext_name, CHARINDEX('(', REVERSE(smarttext_name))) ,')', ''),'(','') END,''),'Unknown') AS Language	  
,COALESCE(NULLIF([THIRD_PARTY_SOURCE],''),'Unknown') as ThirdPartySource
,[CreatedDate]
